@model UNIVERSE.Models.Creature
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Privilege Level: @Model.Privilege.privilegeName</li> 
  @if (Model.Birthdate.HasValue)
  {<li>
      Date of Birth: @Model.Birthdate
    </li>}
</ul>

